{
  "name": "@rnx-kit/typescript-react-native-compiler",
  "version": "0.0.1",
  "description": "TypeScript compiler with a custom module resolver for react-native projects",
  "homepage": "https://github.com/microsoft/rnx-kit/tree/main/packages/typescript-react-native-compiler#readme",
  "license": "MIT",
  "files": [
    "lib/*"
  ],
  "main": "lib/index.js",
  "bin": {
    "rn-tsc": "lib/rn-tsc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rnx-kit",
    "directory": "packages/typescript-react-native-compiler"
  },
  "scripts": {
    "build": "rnx-kit-scripts build",
    "format": "rnx-kit-scripts format",
    "lint": "rnx-kit-scripts lint",
    "test": "rnx-kit-scripts test"
  },
  "dependencies": {
    "@rnx-kit/typescript-react-native-resolver": "^0.0.1",
    "@rnx-kit/typescript-service": "^1.3.4",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "@rnx-kit/jest-preset": "*",
    "@rnx-kit/tools-react-native": "*",
    "@types/node": "^14.15.0",
    "@types/yargs": "^16.0.0",
    "jest-extended": "^0.11.5",
    "rnx-kit-scripts": "*"
  },
  "depcheck": {
    "ignoreMatches": [
      "@rnx-kit/jest-preset"
    ]
  },
  "eslintConfig": {
    "extends": "@rnx-kit/eslint-config"
  },
  "jest": {
    "preset": "@rnx-kit/jest-preset",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "roots": [
      "test"
    ],
    "testRegex": "/test/.*\\.test\\.ts$"
  }
}
